<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dto.UnplannedDeal" table="amirus_unplanned" entity-name="UnplannedDeal">
		<id name="id" type="long">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="market" type="string" column="market" />
		<property name="month1" type="string" column="month1" />
		<property name="month2" type="string" column="month2" />
		<property name="year1" type="int" column="year1" />
		<property name="year2" type="int" column="year2" />
		<property name="instrument" type="string" column="instrument" />
		<property name="direction" type="boolean" column="direction" />
		<property name="start" type="long" column="start" />
		<property name="end" type="long" column="end" />
		<property name="pips" type="double" column="pips" />
		<property name="factor" type="double" column="factor" />
		<property name="currentPrice" type="double" column="currentprice" />
		<property name="startPrice" type="double" column="startprice" />
		<property name="endPrice" type="double" column="endprice" />
		<property name="startPriceMode" type="boolean" column="startprice_manual" />
		<property name="endPriceMode" type="boolean" column="endprice_manual" />
		<property name="benchmarkLots" type="int" column="benchmark_lots" />
		<property name="accountName" type="string" column="account" />
		<set name="orders" table="amirus_orders" lazy="false" cascade="save-update">
			<key>
				<column name="unplanned_id" not-null="false" />
			</key>
			<one-to-many entity-name="Order" />
		</set>
		<property name="archived" type="boolean" column="archived" />
		<property name="year" type="int" column="year" />
		<property name="priceChart" type="java.io.Serializable" column="pricechart" />
		<property name="startEndPriceChart" type="java.io.Serializable" column="start_end_pricechart" />
		<property name="urlPriceChart" type="java.io.Serializable" column="url_pricechart" />
	</class>
</hibernate-mapping>