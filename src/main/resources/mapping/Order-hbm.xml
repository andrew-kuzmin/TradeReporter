<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dto.Order" table="amirus_orders" entity-name="Order">
	<id name="id" type="long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="submitTime" type="long" column="submitTime" />
		<property name="instrument" type="string" column="instrument" />
		<property name="buy" type="boolean" column="buy" />
		<property name="lots" type="integer" column="lots" />
		<property name="price" type="double" column="price" />
		<property name="account" type="string" column="account" />
		<property name="deleted" type="boolean" column="deleted" />
		<property name="plannedDealId" type="long" column="planned_id"></property>
		<property name="unplannedDealId" type="long" column="unplanned_id"></property>
		<property name="messageNumber" type="int" column="message_number"></property>
		<property name="message" type="string" column="message"></property>
	</class>
</hibernate-mapping>